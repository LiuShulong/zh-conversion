    "description": "简体和繁体转换工具",
    "license": "MIT",
    "keywords": ["简体", "繁体", "转换"],
    "authors": [
        {
            "name": "LiuShulong",
            "email": "bigfish_liu@126.com"
        }
    ],
    "require": {
        "php" : ">=7.0.10"
    },
    "require-dev": {
